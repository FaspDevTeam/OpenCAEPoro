{
    "private": true,
    "name": "OpenCAEPoro",
    "version": "1.0.0",
    "packageManager": "pnpm@7.11.0",
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "doc:dev": "pnpm doc:build:markdown && pnpm --filter opencaeporo-doc doc:dev",
        "doc:build:pdf": "bash ./scripts/doc/build/latex.sh",
        "doc:build:markdown": "bash ./scripts/doc/build/markdown.sh",
        "doc:build:vitepress": "pnpm --filter opencaeporo-doc install && pnpm doc:build:markdown && bash ./scripts/doc/build/vitepress.sh",
        "doc:publish:render": "bash ./scripts/doc/publish/render.sh"
    }
}