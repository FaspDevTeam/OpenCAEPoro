{
    "private": true,
    "name": "OpenCAEPoro",
    "version": "0.2.0",
    "packageManager": "pnpm@7.11.0",
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "test:linux:gnu": "bash ./scripts/test/unix.sh linux gnu Release",
        "test:linux:intel": "bash ./scripts/test/unix.sh linux intel Release",
        "test:mac:intel": "bash ./scripts/test/unix.sh mac intel Release",
        "test:windows:intel": "call scripts/test/windows.bat intel Release",
        "build:linux:gnu": "bash ./scripts/build/unix.sh linux gnu Debug",
        "build:linux:intel": "bash ./scripts/build/unix.sh linux intel Debug",
        "build:mac:intel": "bash ./scripts/build/unix.sh mac intel Debug",
        "build:windows:intel": "call scripts/build/windows.bat intel Debug",
        "doc:dev": "pnpm doc:build:markdown && pnpm --filter opencaeporo-doc install && pnpm --filter opencaeporo-doc doc:dev",
        "doc:build:doxygen": "bash ./scripts/doc/build/doxygen.sh",
        "doc:build:markdown": "pnpm doc:build:doxygen && bash ./scripts/doc/build/markdown.sh",
        "doc:build:sphinx": "bash ./scripts/doc/build/sphinx.sh",
        "doc:build:vitepress": "pnpm --filter opencaeporo-doc install && pnpm doc:build:markdown && bash ./scripts/doc/build/vitepress.sh",
        "doc:publish:render": "bash ./scripts/doc/publish/render.sh"
    }
}