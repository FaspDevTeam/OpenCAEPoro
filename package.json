{
    "private": true,
    "name": "OpenCAEPoro",
    "version": "0.2.0",
    "packageManager": "pnpm@7.11.0",
    "engines": {
        "node": ">=14.0.0"
    },
    "scripts": {
        "build:linux:gnu:debug": "bash ./scripts/build/linux/gnu-build.sh Debug",
        "build:linux:gnu:debug:clean": "bash ./scripts/build/linux/clean.sh gnu Debug",
        "build:linux:gnu:release": "bash ./scripts/build/linux/gnu-build.sh Release",
        "build:linux:gnu:release:clean": "bash ./scripts/build/linux/clean.sh gnu Release",
        "build:windows:intel:debug": "call scripts/build/windows/intel-build.bat Debug",
        "build:windows:intel:debug:clean": "call scripts/build/windows/clean.bat intel Debug",
        "build:mac:intel:debug": "bash ./scripts/build/mac/intel-build.sh Debug",
        "build:mac:intel:debug:clean": "bash ./scripts/build/mac/clean.sh intel Debug",
        "doc:dev": "pnpm doc:build:markdown && pnpm --filter opencaeporo-doc install && pnpm --filter opencaeporo-doc doc:dev",
        "doc:build:doxygen": "bash ./scripts/doc/build/doxygen.sh",
        "doc:build:markdown": "pnpm doc:build:doxygen && bash ./scripts/doc/build/markdown.sh",
        "doc:build:vitepress": "pnpm --filter opencaeporo-doc install && pnpm doc:build:markdown && bash ./scripts/doc/build/vitepress.sh",
        "doc:publish:render": "bash ./scripts/doc/publish/render.sh"
    }
}